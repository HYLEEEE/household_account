<table data-role="table" data-mode="columntoggle" class="ui-responsive ui-shadow" id="dataTable">
    <thead>
        <tr>
            <th data-priority="12">적금ID</th>
            <th data-priority="13">예/적금 종류</th>
            <th>예/적금 이름</th>
            <th data-priority="1">계좌이름</th>
            <th data-priority="2">계좌번호</th>
            <th data-priority="3">은행</th>
            <th data-priority="4">이율종류</th>
            <th data-priority="6">이율</th>
            <th data-priority="7">기간</th>
            <th data-priority="8">만기일</th>
            <th data-priority="9">월단위 정액</th>
            <th data-priority="10">누계</th>
            <th data-priority="11">연 누계</th>
            <th data-priority="4">내용</th>
        </tr>
    </thead>
    <tbody>
        <% dataList.forEach(function(item, index) { %>
        <tr>
            <td><%= item.id %></td>
            <td><%= item.type %></td>
            <td><%= item.name %></td>
            <td><%= item.bank_account_name %></td>
            <td><%= item.account_number %></td>
            <td><%= item.bank_name %></td>
            <td>
                <% if(item.rate_type == 0){ %>
                    정액적립식
                <% } else if(item.rate_type == 1){ %>
                    자유적립식
                <% } %>
            </td>
            <td><%= item.rate %> %</td>
            <td><%= item.term/12 +'년(' + item.term + '개월)' %> </td>
            <td><%= item.expiration_date %></td>
            <td><%= item.monthly_price %> 원</td>
            <td><%= item.total_price %> 원</td>
            <td><%= item.year_total_price %> 원</td>
            <td><%= item.contents %></td>
        </tr>
        <%}); %>

    </tbody>
</table>